{
  "name": "device-auth",
  "version": "1.1.10",
  "description": "A simple library for authenticating with just one click, and it's more secure than a password!",
  "repository": {
    "type": "git",
    "url": "https://github.com/zlenner/device-auth"
  },
  "license": "ISC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite --host --open",
    "build": "vite build --config vite.config.ts && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "build:demo": "vite build --config demo.vite.config.ts",
    "deploy-docs": "npm run build:demo && gh-pages -d demo-dist",
    "pub": "npm run build && npm publish --access public",
    "watch": "vite build --watch",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix"
  },
  "dependencies": {
    "@passwordless-id/webauthn": "^1.2.4",
    "dip93": "^1.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react": "^3.0.0",
    "dts-bundle-generator": "^7.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
